<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity Comment.
    -->
    <changeSet id="20201209165359-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="property_id"
                                 baseTableName="comment"
                                 constraintName="fk_comment_property_id"
                                 referencedColumnNames="id"
                                 referencedTableName="property"/>

        <addForeignKeyConstraint baseColumnNames="news_id"
                                 baseTableName="comment"
                                 constraintName="fk_comment_news_id"
                                 referencedColumnNames="id"
                                 referencedTableName="news"/>

    </changeSet>
</databaseChangeLog>
